<div class='container'>
  <h1>Lists</h1>
  <ul clas='list-group'>
    <% @lists.each do |list| %>
      <li class='list-group-item'>
        <%= link_to list.name, list_path(list) %>
      </li>
    <% end %>

  </ul>
  <%= link_to "Create a new list", new_list_path, :class => "btn btn-primary" %>
</div>
